# -*- mode: sh -*-
# vim: set ft=sh:
# emacs ;<tab>
function ___fzf_complete_emacs() {

  _fzf_complete '-m' "$@" < <(
        zpwrRecentfThenNvim | perl -le '@l=reverse<>;@u=do{my %seen;grep{!$seen{$_}++}@l};for(@u){do{$o=$1;($f=$1)=~s@~@$ENV{HOME}@;print $o if -f $f}if m{^>.(.*)}}'
    )
}

___fzf_complete_emacs "$@"
