function ___clearTemp(){

    {
        command rm -rf \
            "$ZPWR_TEMPFILE" \
            "$ZPWR_TEMPFILE1" \
            "$ZPWR_TEMPFILE2" \
            "$ZPWR_TEMPFILE3" \
            "$ZPWR_TEMPFILE4" \
            "$ZPWR_TEMPFILE_SQL" \
            "$ZPWR_TMUX_LOCAL"/pane* \
            "$ZPWR_HIDDEN_DIR_TEMP"/*
    } >> "$ZPWR_LOGFILE" 2>&1
}

___clearTemp "$@"
