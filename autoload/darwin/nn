# -*- mode: sh -*-
# vim: set ft=sh:
function ___nn(){

        if [[ -z "$2" ]];then
            loggErr "usage: nn <title> <message>..." &&
            return 1
        fi

        local title msg

        title="$1"
        msg="$2"
        echo "display notification \"$msg\" with title \"$title\"" |
            osascript
    }

___nn "$@"
