function ___vimAllEdit(){

    BUFFER="$(fzvimAll)"
    if [[ -z "$BUFFER" ]]; then
        return
    fi
    BUFFER="$EDITOR $BUFFER"
    loggDebug "builtin cd $ZPWR"
    eval "builtin cd $ZPWR"

    BUFFER="$BUFFER; clearList; isGitDir && git diff HEAD"
    loggDebug "$BUFFER"
    print -s -- "$BUFFER"
    eval "$BUFFER"
}

___vimAllEdit "$@"
