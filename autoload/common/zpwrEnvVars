# -*- mode: sh -*-
# vim: set ft=sh:
function ___zpwrEnvVars(){

    env | command grep -i "^$ZPWR_REPO_NAME" |
        FZF_DEFAULT_OPTS="--height ${FZF_TMUX_HEIGHT:-50%} --min-height 15 --reverse $FZF_DEFAULT_OPTS $FZF_COMPLETION_OPTS --preview 'echo {}' --preview-window down:3:wrap" __fzf_comprun -m
}

___zpwrEnvVars "$@"
