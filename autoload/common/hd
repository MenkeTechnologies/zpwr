# -*- mode: sh -*-
# vim: set ft=sh:
function ___hd(){

    local cmd

    if cmd="$(getGHDelURL)";then

        prettyPrint "Exe '$cmd'"
        prettyPrintNoNewline "are you sure? y/n > "
        read
        if [[ $REPLY == y ]]; then
            if eval "$cmd"; then
                prettyPrint "Successful deletion."
            else
                loggErr "Error in deletion."
            fi
        fi
    else
            loggErr "getGHDelURL failed."
    fi

}

___hd "$@"
