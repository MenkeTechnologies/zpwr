# -*- mode: sh -*-
# vim: set ft=sh:
function ___hd(){

    local cmd

    if cmd="$(zpwrGetGHDelURL)";then

        if zpwrConfirm "Exe '$cmd'"; then
            if eval "$cmd"; then
                prettyPrint "Successful deletion."
            else
                zpwrLoggErr "Error in deletion."
            fi
        fi
    else
            zpwrLoggErr "zpwrGetGHDelURL failed."
    fi

}

___hd "$@"
