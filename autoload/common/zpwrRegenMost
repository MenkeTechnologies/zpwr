# -*- mode: sh -*-
# vim: set ft=sh:
function zpwrRegenMost(){

    zpwrRegenConfigLinks
    zpwrRegenZshCompCache
    zpwrRegenHistory
    zpwrRegenCtags
    zpwrRegenGtagsCtags
    zpwrRegenAllKeybindingsCache
    zpwrRegenPowerlineLink
    zpwrRegenSearchEnv
    zpwrRefreshZwc
}

zpwrRegenMost "$@"
