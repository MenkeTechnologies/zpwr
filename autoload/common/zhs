# -*- mode: sh -*-
# vim: set ft=sh:
function zhs() {

    if [[ -d "$ZPWR_PLUGIN_MANAGER_HOME/snippets" ]]; then
        zpwrCd "$ZPWR_PLUGIN_MANAGER_HOME/snippets"
    else
        zpwrLogConsoleErr "ZPWR_PLUGIN_MANAGER_HOME/snippets '$ZPWR_PLUGIN_MANAGER_HOME/snippets' is not a dir."
    fi

}

zhs "$@"
