# -*- mode: sh -*-
# vim: set ft=sh:
function zpwrLoadJenv() {

    if exists jenv;then
        eval "$(jenv init -)"
        jenv enable-plugin export > /dev/null 2>&1
        zpwrPrettyPrint "jenv loaded"
        if [[ -n "$1" ]]; then
            jenv "$@"
        fi
    else
        zpwrLoggErr "No jenv"
    fi
}

zpwrLoadJenv "$@"
