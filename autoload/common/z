# -*- mode: sh -*-
# vim: set ft=sh:
function z(){

    if [[ -z ${ZPWR_VARS[zcmd]} ]]; then
        local zcmd
        if exists zshz; then
            ZPWR_VARS[zcmd]="zshz"
        elif exists _z; then
            ZPWR_VARS[zcmd]="_z 2>&1"
        else
            loggErr "ERROR: zshz and _z do not exist."
            return 1
        fi
    fi
    # force alias z to zshz not zypper on suse
    ${=ZPWR_VARS[zcmd]} "$@"

}

z "$@"
