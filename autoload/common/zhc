# -*- mode: sh -*-
# vim: set ft=sh:
function zhc() {

    if [[ -d "$ZPWR_PLUGIN_MANAGER_HOME/completions" ]]; then
        cd "$ZPWR_PLUGIN_MANAGER_HOME/completions"
    else
        zpwrLoggErr "ZPWR_PLUGIN_MANAGER_HOME/completions '$ZPWR_PLUGIN_MANAGER_HOME/completions' is not a dir."
    fi

}

zhc "$@"
