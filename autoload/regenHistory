function ___regenHistory() {

    prettyPrint "Regen $HISTFILE"
    (
        builtin cd "$HOME"
        command mv "$HISTFILE" .zsh_history_bad
        command strings .zsh_history_bad > "$HISTFILE"
        builtin fc -R ."$HISTFILE"
        command rm -rf .zsh_history_bad
    )
}

___regenHistory "$@"
