#select the first (0) pane
selectp -t 0
# split it into two vertical halves
splitw -h -p 50
#select the first (0) pane
selectp -t 1
# split it into two horizontal halves
splitw -v -p 50

#select last created pane
selectp -t 2

wait-for -L my1
#if se does not exist like wth zinit late load, have tmux sleep
send-keys "if exists se; then  bash -c 'tmux wait-for -U my1' & else bash -c 'sleep 8; tmux wait-for -U my1' & fi" C-m
wait-for -L my1

#proceed after sleep
selectp -t 0
send-keys "se" C-m

selectp -t 1
send-keys "sef" C-m

selectp -t 2
send-keys "se; zpwrStartSend 1" C-m
